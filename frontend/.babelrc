{
  "env": {
    "development": {
      "presets": ["@babel/env", "@babel/typescript", "@babel/react"],
      "plugins": ["@babel/proposal-class-properties", "babel-plugin-typescript-to-proptypes"]
    },
    "test": {
      "presets": ["@babel/env", "@babel/typescript", "@babel/react"],
      "plugins": ["@babel/proposal-class-properties"]
    },
    "production": {
      "presets": ["@babel/env", "@babel/typescript", "@babel/react"],
      "plugins": ["@babel/proposal-class-properties"]
    }
  }
}
